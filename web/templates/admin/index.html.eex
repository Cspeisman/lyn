<h2>Listing</h2>

<table class="table">
  <thead>
    <tr>
      <%= for {id, column} <- @columns do %>
        <th><%= column[:label] %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
<%= for entry <- @entries do %>
    <tr>
      <%= for {id, column} <- @columns do %>
        <td><%= Map.get(entry, id, nil) %></td>
      <% end %>
    </tr>
<% end %>
  </tbody>
</table>
